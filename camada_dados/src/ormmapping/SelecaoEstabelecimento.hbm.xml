<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="SelecaoEstabelecimento, Base de Dados - MM" table="SelecaoEstabelecimento" lazy="false">
		<composite-id>
			<key-property name="Id_avaliacao" column="id_avaliacao" type="Int32"/>
			<key-many-to-one name="ORM_Estabelecimento" class="Cliente_seleciona_Estabelecimento, Base de Dados - MM">
				<column name="Estabelecimento"/>
				<column name="Cliente"/>
			</key-many-to-one>
		</composite-id>
		<property name="Data_hora_selecao" column="data_hora_selecao" type="Timestamp" not-null="true"/>
	</class>
</hibernate-mapping>
