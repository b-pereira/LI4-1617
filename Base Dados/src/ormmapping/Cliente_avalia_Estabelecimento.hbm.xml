<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Cliente_avalia_Estabelecimento, Base de Dados - MM" table="Cliente_avalia_Estabelecimento" lazy="false">
		<composite-id>
			<key-many-to-one name="ORM_Estabelecimento" column="Estabelecimento" class="Estabelecimento, Base de Dados - MM">
			</key-many-to-one>
			<key-many-to-one name="ORM_Cliente" column="Cliente" class="Cliente, Base de Dados - MM">
			</key-many-to-one>
		</composite-id>
		<property name="Rating_est" column="rating_est" type="Single" length="10" not-null="true"/>
		<property name="Data_avaliacao" column="data_avaliacao" type="Timestamp" not-null="true"/>
	</class>
</hibernate-mapping>
