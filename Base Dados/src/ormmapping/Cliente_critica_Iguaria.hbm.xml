<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Cliente_critica_Iguaria, Base de Dados - MM" table="Cliente_critica_Iguaria" lazy="false">
		<composite-id>
			<key-many-to-one name="ORM_Cliente" column="Cliente" class="Cliente, Base de Dados - MM">
			</key-many-to-one>
			<key-many-to-one name="ORM_Iguaria" class="Iguaria, Base de Dados - MM">
				<column name="Iguaria"/>
				<column name="Estabelecimento"/>
			</key-many-to-one>
		</composite-id>
		<property name="Desc_critica" column="desc_critica" type="String" length="255" not-null="true"/>
		<property name="Data_critica" column="data_critica" type="Timestamp" not-null="true"/>
		<property name="Rating_igu" column="rating_igu" type="Single" length="10" not-null="true"/>
	</class>
</hibernate-mapping>
