<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Cliente_seleciona_Estabelecimento, Base de Dados - MM" table="Cliente_seleciona_Estabelecimento" lazy="false">
		<composite-id>
			<key-many-to-one name="ORM_Estabelecimento" column="Estabelecimento" class="Estabelecimento, Base de Dados - MM">
			</key-many-to-one>
			<key-many-to-one name="ORM_Cliente" column="Cliente" class="Cliente, Base de Dados - MM">
			</key-many-to-one>
		</composite-id>
		<set name="ORM_SelecaoEstabelecimento" lazy="true" cascade="save-update" inverse="true">
			<key>
				<column name="Estabelecimento"/>
				<column name="Cliente"/>
			</key>
			<one-to-many class="SelecaoEstabelecimento, Base de Dados - MM"/>
		</set>
	</class>
</hibernate-mapping>
