<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Cliente_seleciona_iguaria, Base de Dados - MM" table="Cliente_seleciona_iguaria" lazy="false">
		<composite-id>
			<key-many-to-one name="ORM_Cliente" column="Cliente" class="Cliente, Base de Dados - MM">
			</key-many-to-one>
			<key-many-to-one name="ORM_Iguaria" class="Iguaria, Base de Dados - MM">
				<column name="Iguaria"/>
				<column name="Estabelecimento"/>
			</key-many-to-one>
		</composite-id>
		<set name="ORM_SelecaoIguaria" lazy="true" cascade="save-update" inverse="true">
			<key>
				<column name="Cliente"/>
				<column name="Iguaria"/>
				<column name="Estabelecimento"/>
			</key>
			<one-to-many class="SelecaoIguaria, Base de Dados - MM"/>
		</set>
	</class>
</hibernate-mapping>
